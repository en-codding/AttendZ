<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.AttendZ" parent="Theme.AppCompat.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/dark_purple_200</item>
        <item name="colorPrimaryVariant">@color/dark_purple_700</item>
        <item name="colorOnPrimary">@color/dark_black</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/dark_teal_200</item>
        <item name="colorSecondaryVariant">@color/dark_teal_200</item>
        <item name="colorOnSecondary">@color/dark_black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
</resources>